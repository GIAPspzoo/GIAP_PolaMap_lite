{
    background: #6FB1E3;
	color:  #EDF6FC;

}

QMenu::icon
{
    position:absolute;
    left: 6px;
}

QWidget:disabled
{
	color:  gray;
	background-color:  rgb(53, 85, 109);
}

QAbstractItemView
{
	alternate-background-color:  #4f5a63;
	color:  #EDF6FC;
	border-radius: 3px;
	font: 9pt "Segoe UI"; /* ortofotomapa */
