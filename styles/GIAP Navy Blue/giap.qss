}


QWidget:focus, QMenuBar:focus
{
	border: 1px solid #5689b0;
}
QScrollArea { border: none;}

QTabWidget:focus, QCheckBox:focus
{
	border: none;
}

QLineEdit
{
    border-radius: 3px;
    background-color: #4f5a63;
}


QLineEdit:focus{
    border: 2px solid #5689B0;
}


QLineEdit:disabled
{
   color: grey;
   border: 2px solid #4f5a63;
   border-radius: 3px;
   padding: 2px;
}

QGroupBox {
    border:1px solid  #5689B0;
    border-radius: 7px;
    padding-left: 5px;
    padding-right: 5px;
    margin-top: 10px;
    margin-bottom: 3px;

}

QGroupBox::title {
	subcontrol-origin: margin;
	subcontrol-position: top left;
	padding-left: 10px;
	padding-right: 10px;
}

QScrollBar:horizontal {
    background-color: #35556D;
    height: 0.5em;
    margin: 0px;
    padding: 0px;
}

QScrollBar::handle:horizontal {
    background: #B7D3E8;
    min-width: 0.8em;
    border-radius: 3px;
    border:None;
}

QScrollBar::add-line:horizontal, QScrollBar::sub-line:horizontal,
QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal {
    width: 0px;
    background: transparent;
}

QScrollBar:vertical {
    background-color: #35556D;
    width: 0.5em;
    margin: 0;
}

QScrollBar::handle:vertical {
    background: #B7D3E8;
    min-height: 0.8em;
    border-radius: 3px;
    border:None;
}

QAbstractScrollArea::corner {
    background: transparent;
    border: none;
}

QAbstractScrollArea QToolButton{
    padding: 3px;
   background-color: transparent;
   border: none;
}

QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical,
QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
    height: 0px;
    background: transparent;
}

QTabBar {
    qproperty-drawBase: 0;
    font: 10pt "Segoe UI"; /* komunikaty wtyczki general */
	font-weight: 600;
}

QDialog QTabBar::tab {
    padding: 2px;
    padding-left: 20px;
    padding-right: 20px;
    height: 20px;
}


QTabBar::tab {
    padding:2px;
    padding-left: 30px;
    padding-right: 5px;
    height: 20px;
}

QTabBar::tab:selected {
    border: 1px solid #5689b0;
    background-color: #5689b0;
}

QTabBar::tab:!selected {
    border: 1px solid transparent;
    color: rgb(203, 212, 220);
}

QTabBar::tab:last:!selected {
    margin-right: 1px;
    border: 1px solid transparent;
}

QTabWidget::pane {
	border-top: 2px solid rgb(79, 118, 150);
	border-bottom: 2px solid rgb(79, 118, 150);
}

QPushButton{
    color:  #EDF6FC;
    background-color: #5689B0;
    border-radius: 3px;
    padding-top: 5px;
    padding-bottom: 5px;
