        }

QgsMessageBar {
    padding: 5px;
}
QgsMessageBar QFrame {
    border: 0px solid;
}
QgsMessageBar QLabel {
    background-color: transparent;
}
QgsMessageBar QToolButton {
    color: black;
    border: 1px solid black;
}
QgsMessageBar QToolButton:hover {
    background-color: transparent;
    color: black;
    border: 2px solid black;
}

QListView::indicator:checked, QWidget QFrame QFrame QTreeView::indicator:checked{
image: url(icons/checked_black.png);
    width: 12px;
    height: 12px;
}

QListView::indicator:unchecked, QWidget QFrame QFrame QTreeView::indicator:unchecked {
image: url(icons/unchecked_black.png);
    width: 12px;
    height: 12px;
}
QListView::indicator:unchecked:hover, QListView::indicator:unchecked:selected, QWidget QFrame QFrame QTreeView::indicator:unchecked:hover, QWidget QFrame QFrame QTreeView::indicator:unchecked:selected {
