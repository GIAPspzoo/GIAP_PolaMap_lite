    width: 12px;
    height: 12px;
}
QListView::indicator:checked:hover, QListView::indicator:checked:selected, QWidget QFrame QFrame QTreeView::indicator:checked:hover, QWidget QFrame QFrame QTreeView::indicator:checked:selected {
